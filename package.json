{
  "name": "boot-ink",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.12.4",
  "description": "_description_",
  "author": "Kirk Lin <https://github.com/kirklin>",
  "license": "MIT",
  "funding": "https://www.buymeacoffee.com/linkirk",
  "homepage": "https://github.com/kirklin/boot-ink#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirklin/boot-ink.git"
  },
  "bugs": "https://github.com/kirklin/boot-ink/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/app.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/app.mjs",
  "module": "./dist/app.mjs",
  "types": "./dist/app.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run build",
    "release": "bumpp",
    "start": "esno src/app.tsx",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "up": "taze latest -r -w -I"
  },
  "dependencies": {
    "ink": "^6.0.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@antfu/ni": "^25.0.0",
    "@kirklin/eslint-config": "^5.0.0",
    "@types/node": "^24.0.6",
    "@types/react": "^19.1.8",
    "bumpp": "^10.2.0",
    "eslint": "^9.30.0",
    "esno": "^4.8.0",
    "lint-staged": "^16.1.2",
    "pnpm": "^10.12.4",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.0",
    "taze": "^19.1.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
